<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carrentalsystem.RentCarController" prefHeight="600.0" prefWidth="800.0">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="Car Name:" />
        <ComboBox fx:id="carNameComboBox" layoutX="150.0" layoutY="50.0" prefWidth="300.0" />
        <Label layoutX="50.0" layoutY="100.0" text="Customer Name:" />
        <ComboBox fx:id="customerNameComboBox" layoutX="150.0" layoutY="100.0" prefWidth="300.0" />
        <Label layoutX="50.0" layoutY="150.0" text="Start Date:" />
        <DatePicker fx:id="startDatePicker" layoutX="150.0" layoutY="150.0" />
        <Label layoutX="50.0" layoutY="200.0" text="End Date:" />
        <DatePicker fx:id="endDatePicker" layoutX="150.0" layoutY="200.0" />
        <Button layoutX="150.0" layoutY="250.0" text="Rent Car" onAction="#handleRentCar" />
        <Button layoutX="250.0" layoutY="250.0" text="Generate Receipt" onAction="#handleGenerateReceipt" />
        <Button layoutX="150.0" layoutY="300.0" text="Back" onAction="#handleBack" />
        <Button layoutX="250.0" layoutY="300.0" text="Logout" onAction="#handleLogout" />
    </children>
</AnchorPane>