<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.carrentalsystem.RentalHistoryController">
    <TableView fx:id="rentalHistoryTable" layoutX="14.0" layoutY="20.0" prefHeight="604.0" prefWidth="960.0">
        <columns>
            <TableColumn fx:id="carNameColumn" prefWidth="240.0" text="Car Name" />
            <TableColumn fx:id="carTypeColumn" prefWidth="240.0" text="Car Type" />
            <TableColumn fx:id="customerNameColumn" prefWidth="240.0" text="Customer Name" />
            <TableColumn fx:id="returnDateColumn" prefWidth="240.0" text="Return Date" />
        </columns>
    </TableView>
    <Button fx:id="backButton" layoutX="14.0" layoutY="651.0" onAction="#handleBack" prefHeight="25.0" prefWidth="75.0" text="Back" />
</AnchorPane>
